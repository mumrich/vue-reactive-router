{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "unplugin-icons/types/vue",
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "mumrich-vue-components": [
        "src/lib/main"
      ]
    },
    "baseUrl": ".",
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
